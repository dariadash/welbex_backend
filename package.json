{
  "name": "welbex_backend",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc-watch --onSuccess \"cross-env NODE_PATH=dist/ node ./dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:up": "tsc && typeorm migration:run -d dist/utils/dataSource.js",
    "migration:down": "tsc && typeorm migration:revert -d dist/utils/dataSource.js",
    "db:seed": "tsc && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.3",
    "tsc-watch": "^6.0.4",
    "typeorm": "^0.3.15",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^5.0.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "ts-node-dev": "^2.0.0"
  }
}
